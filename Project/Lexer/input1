/* input1 
 * 
 * a "stupid" program used as input file
 * to verify that ALL LEGAL TOKENS can be 
 * recognized by generated scanner, as well as
 * that single-line comments are stripped out
 * (see input1.out for results)
 *
 * author: Huizhe Li, Master Student, Dartmouth College
 *
 */

/* '#', '<', '>', '.', 'ID' */
#include <stdio.h>				

extern int integer;				/* 'extern', 'int', 'ID' */

int main(void){					/* 'VOID' */
	static const int array[3] = {0,0,0};	/* 'static', 'const', 'int', '[', ']', '=', '{', '}', ';', 'NUM', 'ID' */
	struct bozo{				/* 'struct', 'double */
		int bo;
		double zo;
	};

	struct bozo bz;
	bz.bo = 1;
	bz.zo -= 1.0;	/* '-=', 'FNUM' */

	integer += sizeof(int);			/* '+=', 'sizeof' */
	integer /= integer * 3 / 4 - 5;		/* '/=', '*', '/', '-' */
	integer *= integer;			/* '*=' */
	if ( (integer < integer + 1) && (integer <= 0)){				/* '<', '+', '&&', '<=', 'if' */
		if ((--integer > -1) || (++integer >= integer)){			/* '--', '>', '||', '++', '>=' */	
				if ((integer == !integer) && (integer != integer)){	/* '==', '!', '!=' */
					printf("---string literal---\nanother line!\n");/* 'STRING' */
					integer = 10%10 + &integer;			/* 'MOD', 'ADDR' */
				}
		}
	}else{										/* 'else' */
		do{									/* 'do'	*/
			switch (integer){						/* 'switch' */
				case 0: 						/* 'case', 'COLON' */
					continue;					/* 'continue' */
				default:						/* 'default' */
			}
			break;								/* 'break' */
		}while(1);								/* 'while' */

	}
	
	return 0;									/* 'return' */
}
	


