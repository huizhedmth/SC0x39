#define	OR		303
#define	NOT		304
#define	PLUSEQ		305
#define	MINUSEQ		306
#define	MULEQ		307
#define	DIVEQ		308
#define	PRECMP		309
#define	ADDR		310
#define DOT		311


/* other tokens */
