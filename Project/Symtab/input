void f();
void g();
void h();

int main()
{	
	/* scope 1 */
	int a;
	int x;
	const int c;
	
	/* scope 2 */
	{
		int a;
		int b;
		const int c;
		double d;
		
		/* scope 3 */
		{
			int x;
			int b;
		}
		
		/* scope 2 */
		x = 1;
	}
	
	/* scope 1 */
	x = 1;

	return 0;
}

void f()
{
	int f_a;
	double f_b;
	if (1) {
		int ff_a;
		int ff_b;	
	}
	a = 1;
}
		
